{
	"name": "sbine/bdgt",
	"description": "Bdgt",
	"license": "MIT",
	"type": "project",
	"require": {
		"laravel/framework": "5.1.*",
		"filp/whoops": "~1.0"
	},
	"require-dev": {
		"fzaninotto/faker": "~1.4.0",
		"mockery/mockery": "~0.9.4",
		"phpunit/phpunit": "4.0.*",
		"phpspec/phpspec": "~2.1"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"Bdgt\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}
